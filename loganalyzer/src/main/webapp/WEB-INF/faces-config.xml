<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <managed-bean>
        <managed-bean-name>richBean</managed-bean-name>
        <managed-bean-class>org.sbk.hobby.loganalyzer.RichBean</managed-bean-class>
        <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    
	<managed-bean>
        <managed-bean-name>logEventListBean</managed-bean-name>
        <managed-bean-class>org.sbk.hobby.loganalyzer.managedbeans.LogEventListBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
       	<managed-property>
			<property-name>logEventList</property-name> 
			<property-class>java.util.List</property-class> 
			<value/>
		</managed-property>
		<managed-property>
			<property-name>logEventListDataModel</property-name> 
			<property-class>org.sbk.hobby.loganalyzer.extendeddatamodel.LogEventListDataModel</property-class> 
			<value/>
		</managed-property>		
		<managed-property>
			<property-name>selectedLogEvent</property-name> 
			<property-class>org.sbk.hobby.loganalyzer.model.LogEvent</property-class> 
			<value/>
		</managed-property>
       	<managed-property>
			<property-name>selection</property-name> 
			<property-class>java.util.Collection</property-class> 
			<value/>
		</managed-property>		
		<managed-property>
			<property-name>tableState</property-name> 
			<property-class>java.lang.String</property-class> 
			<value/>
		</managed-property>
		<managed-property>
			<property-name>selectedLogFileName</property-name> 
			<property-class>java.lang.String</property-class> 
			<value/>
		</managed-property>
		<managed-property>
			<property-name>uploadedFileNames</property-name> 
			<property-class>java.util.Set</property-class> 
			<value/>
		</managed-property>
    </managed-bean>
</faces-config>
